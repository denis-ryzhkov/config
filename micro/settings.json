{
    "autoclose": false,
    "colorcolumn": 88,
    "colorscheme": "darcula",
    "diffgutter": true,
    "ft:dart": {
        "colorcolumn": 80,
        "tabsize": 2
    },
    "ft:html": {
        "tabsize": 2
    },
    "ft:json": {
        "tabsize": 2
    },
    "ft:yaml": {
        "tabsize": 2
    },
    "linter": false,
    "parsecursor": true,
    "pluginchannels": [
        "https://raw.githubusercontent.com/micro-editor/plugin-channel/master/channel.json"
    ],
    "pluginrepos": [],
    "ruler": false,
    "savecursor": true,
    "savehistory": false,
    "statusformatl": "L$(line)/$(status.lines) C$(col)",
    "statusformatr": "$(status.branch) $(filename) $(modified)",
    "tabsize": 4,
    "tabstospaces": true
}
